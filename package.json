{
  "name": "vueify-example",
  "version": "1.0.0",
  "description": "Vueify transform for Browserify example",
  "main": "index.js",
  "scripts": {
    "dev": "budo src/main.js:build/build.js -- -p browserify-hmr -t vueify",
    "build": "NODE_ENV=production browserify src/main.js -t vueify | uglifyjs -c warnings=false -m > build/build.js"
  },
  "author": "Evan You",
  "license": "MIT",
  "devDependencies": {
    "babel-runtime": "^5.8.25",
    "browserify": "^12.0.1",
    "browserify-hmr": "^0.3.1",
    "budo": "^6.0.1",
    "jade": "^1.11.0",
    "stylus": "^0.52.4",
    "uglify-js": "^2.5.0",
    "vue-hot-reload-api": "^1.2.1",
    "vueify": "^5.0.0",
    "vueify-insert-css": "^1.0.0"
  },
  "dependencies": {
    "vue": "^1.0.0"
  }
}
